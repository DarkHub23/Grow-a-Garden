			Pet = { "‡∏ä‡∏∑‡πà‡∏≠‡∏™‡∏±‡∏ï‡∏ß‡πå‡πÄ‡∏•‡∏µ‡πâ‡∏¢‡∏á", "‡∏ô‡πâ‡∏≥‡∏´‡∏ô‡∏±‡∏Å", "‡∏≠‡∏≤‡∏¢‡∏∏","‡∏Å‡∏≤‡∏£‡∏Å‡∏•‡∏≤‡∏¢‡∏û‡∏±‡∏ô‡∏ò‡∏∏‡πå" },
			Seed = { "‡∏ä‡∏∑‡πà‡∏≠‡πÄ‡∏°‡∏•‡πá‡∏î" },
