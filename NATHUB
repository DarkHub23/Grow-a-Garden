-- Tabs and Content
local tabNames = {"Pet", "Seed", "Egg","Mutation"}
local tabButtons, tabContents, uiRefs = {}, {}, {}

-- ‡∏™‡∏£‡πâ‡∏≤‡∏á‡πÅ‡∏ó‡πá‡∏ö‡πÅ‡∏ö‡∏ö‡πÄ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡πÑ‡∏î‡πâ
local tabFrame = Instance.new("ScrollingFrame", mainFrame)
tabFrame.Size = UDim2.new(1, -20, 0, 40)
tabFrame.Position = UDim2.new(0, 10, 0, 60)
tabFrame.BackgroundTransparency = 1
tabFrame.BorderSizePixel = 0
tabFrame.CanvasSize = UDim2.new(0, #tabNames * 130, 0, 0)
tabFrame.ScrollBarThickness = 4
tabFrame.ScrollingDirection = Enum.ScrollingDirection.X
tabFrame.AutomaticCanvasSize = Enum.AutomaticSize.X

-- ‡∏à‡∏±‡∏î‡∏õ‡∏∏‡πà‡∏°‡πÅ‡∏ô‡∏ß‡∏ô‡∏≠‡∏ô‡πÅ‡∏ö‡∏ö‡∏°‡∏µ‡∏£‡∏∞‡∏¢‡∏∞‡∏´‡πà‡∏≤‡∏á
-- ‡πÄ‡∏û‡∏¥‡πà‡∏° UIListLayout ‡πÉ‡∏´‡πâ‡∏Å‡∏±‡∏ö tabFrame
local layout = Instance.new("UIListLayout", tabFrame)
layout.FillDirection = Enum.FillDirection.Horizontal
layout.HorizontalAlignment = Enum.HorizontalAlignment.Left
layout.SortOrder = Enum.SortOrder.LayoutOrder
layout.Padding = UDim.new(0, 10) -- ‡πÄ‡∏ß‡πâ‡∏ô‡∏´‡πà‡∏≤‡∏á 10px ‡∏£‡∏∞‡∏´‡∏ß‡πà‡∏≤‡∏á‡∏õ‡∏∏‡πà‡∏°

-- ‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏õ‡∏∏‡πà‡∏°‡πÅ‡∏ó‡πá‡∏ö
for i, name in ipairs(tabNames) do
	local btn = Instance.new("TextButton", tabFrame)
	btn.Size = UDim2.new(0, 120, 1, 0)
	btn.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
	btn.Font = Enum.Font.Gotham
	btn.TextSize = 14
	btn.TextColor3 = Color3.fromRGB(255, 255, 255)
	btn.Text = languages[currentLang].tabs[name]
	btn.Name = name
	Instance.new("UICorner", btn).CornerRadius = UDim.new(0, 6)

	btn.MouseButton1Click:Connect(function()
		switchTab(name)
	end)
end


-- ‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏õ‡∏∏‡πà‡∏°‡πÅ‡∏ó‡πá‡∏ö‡∏ï‡∏≤‡∏°‡∏ä‡∏∑‡πà‡∏≠‡πÉ‡∏ô tabNames
for i, name in ipairs(tabNames) do
	local btn = Instance.new("TextButton", tabFrame)
	btn.Size = UDim2.new(0, 120, 1, 0)
	btn.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
	btn.Font = Enum.Font.Gotham
	btn.TextSize = 14
	btn.TextColor3 = Color3.fromRGB(255, 255, 255)
	btn.Text = languages[currentLang].tabs[name]
	btn.Name = name
	Instance.new("UICorner", btn).CornerRadius = UDim.new(0, 6)

	btn.MouseButton1Click:Connect(function()
		switchTab(name)
	end)
end


local contentHolder = Instance.new("Frame", mainFrame)
contentHolder.Size = UDim2.new(1, -20, 1, -120)
contentHolder.Position = UDim2.new(0, 10, 0, 110)
contentHolder.BackgroundTransparency = 1

-- Drag System
local UIS = game:GetService("UserInputService")
local dragging, dragInput, dragStart, startPos
mainFrame.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
		dragging = true
		dragStart = input.Position
		startPos = mainFrame.Position

		input.Changed:Connect(function()
			if input.UserInputState == Enum.UserInputState.End then
				dragging = false
			end
		end)
	end
end)

mainFrame.InputChanged:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
		dragInput = input
	end
end)

UIS.InputChanged:Connect(function(input)
	if input == dragInput and dragging then
		local delta = input.Position - dragStart
		mainFrame.Position = UDim2.new(
			startPos.X.Scale,
			startPos.X.Offset + delta.X,
			startPos.Y.Scale,
			startPos.Y.Offset + delta.Y
		)
	end
end)


-- Loading Screen
local function showLoading()
	local StarterGui = game:GetService("StarterGui")
	local UIS = game:GetService("UserInputService")
	local TweenService = game:GetService("TweenService")
	local player = game.Players.LocalPlayer

	-- ‡∏õ‡∏¥‡∏î GUI ‡πÅ‡∏•‡∏∞ Mouse
	pcall(function()
		StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.All, false)
	end)
	UIS.MouseIconEnabled = false
	UIS.MouseBehavior = Enum.MouseBehavior.LockCenter

	-- ‡∏õ‡∏¥‡∏î‡∏Å‡∏≤‡∏£‡∏Ñ‡∏ß‡∏ö‡∏Ñ‡∏∏‡∏°
	local Controls = require(player:WaitForChild("PlayerScripts"):WaitForChild("PlayerModule")):GetControls()
	Controls:Disable()

	-- ‚úÖ ‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏´‡∏ô‡πâ‡∏≤‡∏à‡∏≠‡πÇ‡∏´‡∏•‡∏î‡πÉ‡∏´‡∏°‡πà
	local loadingGui = Instance.new("ScreenGui", player:WaitForChild("PlayerGui"))
	loadingGui.Name = "LoadingScreen"
	loadingGui.IgnoreGuiInset = true
	loadingGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
	loadingGui.DisplayOrder = 9999 -- ‡∏Ñ‡∏£‡∏≠‡∏ö‡∏à‡∏≠‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î

	-- ‚úÖ ‡∏û‡∏∑‡πâ‡∏ô‡∏´‡∏•‡∏±‡∏á‡∏î‡∏≥
	local bg = Instance.new("Frame", loadingGui)
	bg.Size = UDim2.new(1, 0, 1, 0)
	bg.BackgroundColor3 = Color3.fromRGB(10, 10, 10)
	bg.ZIndex = 9999

	-- ‚úÖ ‡πÄ‡∏Ñ‡∏£‡∏î‡∏¥‡∏ï
	local loadingCredit = Instance.new("TextLabel", bg)
	loadingCredit.AnchorPoint = Vector2.new(1, 1)
	loadingCredit.Position = UDim2.new(1, -10, 1, -10)
	loadingCredit.Size = UDim2.new(0, 200, 0, 20)
	loadingCredit.BackgroundTransparency = 1
	loadingCredit.Font = Enum.Font.Gotham
	loadingCredit.TextColor3 = Color3.fromRGB(120, 120, 120)
	loadingCredit.TextSize = 12
	loadingCredit.TextXAlignment = Enum.TextXAlignment.Right
	loadingCredit.Text = "Made by dkubpom_"
	loadingCredit.ZIndex = 10000

	-- ‚úÖ ‡∏Ñ‡∏≥‡πÄ‡∏ï‡∏∑‡∏≠‡∏ô
	local warnLabel = Instance.new("TextLabel", bg)
	warnLabel.AnchorPoint = Vector2.new(0.5, 0)
	warnLabel.Position = UDim2.new(0.5, 0, 0.5, 40)
	warnLabel.Size = UDim2.new(0, 400, 0, 40)
	warnLabel.BackgroundTransparency = 1
	warnLabel.Font = Enum.Font.Gotham
	warnLabel.TextColor3 = Color3.fromRGB(255, 80, 80)
	warnLabel.TextSize = 16
	warnLabel.TextWrapped = true
	warnLabel.Text = currentLang == "en"
		and "Do not leave the game, or you may lose your items."
		or "‡∏´‡πâ‡∏≤‡∏°‡∏≠‡∏≠‡∏Å‡∏à‡∏≤‡∏Å‡πÄ‡∏Å‡∏° ‡∏°‡∏¥‡∏â‡∏∞‡∏ô‡∏±‡πâ‡∏ô‡∏Ñ‡∏∏‡∏ì‡∏≠‡∏≤‡∏à‡∏™‡∏π‡∏ç‡πÄ‡∏™‡∏µ‡∏¢‡∏Ç‡∏≠‡∏á!"
	warnLabel.ZIndex = 10000

	-- ‚úÖ ‡∏Å‡∏£‡∏∞‡∏û‡∏£‡∏¥‡∏ö‡πÄ‡∏ï‡∏∑‡∏≠‡∏ô
	local tweenInfo = TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut, -1, true)
	local fadeTween = TweenService:Create(warnLabel, tweenInfo, { TextTransparency = 0.4 })
	fadeTween:Play()

	-- ‚úÖ ‡πÄ‡∏õ‡∏≠‡∏£‡πå‡πÄ‡∏ã‡πá‡∏ô‡∏ï‡πå‡πÇ‡∏´‡∏•‡∏î
	local percentLabel = Instance.new("TextLabel", bg)
	percentLabel.AnchorPoint = Vector2.new(0.5, 0.5)
	percentLabel.Position = UDim2.new(0.5, 0, 0.5, 0)
	percentLabel.Size = UDim2.new(0, 200, 0, 60)
	percentLabel.BackgroundTransparency = 1
	percentLabel.Font = Enum.Font.GothamBlack
	percentLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
	percentLabel.TextSize = 36
	percentLabel.ZIndex = 10000
	percentLabel.Text = ""

	-- ‚úÖ ‡πÄ‡∏£‡∏¥‡πà‡∏°‡πÇ‡∏´‡∏•‡∏î‡πÅ‡∏ö‡∏ö‡∏õ‡∏•‡∏≠‡∏°
	for i = 1, 100 do
		task.wait(0.7)
		percentLabel.Text = "Loading: " .. i .. "%"
	end

	-- ‚úÖ ‡πÇ‡∏´‡∏•‡∏î‡πÄ‡∏™‡∏£‡πá‡∏à
	task.wait(2)
	loadingGui:Destroy()
	Controls:Enable()
	UIS.MouseBehavior = Enum.MouseBehavior.Default
	UIS.MouseIconEnabled = true
	pcall(function()
		StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.All, true)
	end)
end


-- Create Tab UI
local function createTab(name)
	local scroll = Instance.new("ScrollingFrame")
	scroll.Size = UDim2.new(1, 0, 1, 0)
	scroll.BackgroundTransparency = 1
	scroll.CanvasSize = UDim2.new(0, 0, 0, 500)
	scroll.ScrollBarThickness = 6
	scroll.Visible = false

	local layout = Instance.new("UIListLayout", scroll)
	layout.SortOrder = Enum.SortOrder.LayoutOrder
	layout.Padding = UDim.new(0, 10)
	layout.HorizontalAlignment = Enum.HorizontalAlignment.Center

	uiRefs[name] = {}
	local fieldCount = #languages[currentLang].fields[name]
	for i = 1, fieldCount do
		local entry = Instance.new("Frame")
		entry.Size = UDim2.new(0, 400, 0, 40)
		entry.AnchorPoint = Vector2.new(0.5, 0)
		entry.Position = UDim2.new(0.5, 0, 0, 0)
		entry.BackgroundTransparency = 1

		local label = Instance.new("TextLabel", entry)
		label.Size = UDim2.new(0.4, 0, 1, 0)
		label.BackgroundTransparency = 1
		label.Font = Enum.Font.Gotham
		label.TextColor3 = Color3.fromRGB(200, 200, 200)
		label.TextSize = 14
		label.TextXAlignment = Enum.TextXAlignment.Left
		label.Text = languages[currentLang].fields[name][i]

		local input = Instance.new("TextBox", entry)
		input.Size = UDim2.new(0.6, -10, 1, -4)
		input.Position = UDim2.new(0.4, 10, 0, 2)
		input.Font = Enum.Font.Gotham
		input.TextColor3 = Color3.fromRGB(255, 255, 255)
		input.TextSize = 14
		input.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
		input.PlaceholderText = languages[currentLang].placeholder
		Instance.new("UICorner", input).CornerRadius = UDim.new(0, 6)

		entry.Parent = scroll
		table.insert(uiRefs[name], { label = label, input = input })
	end

	local btn = Instance.new("TextButton", scroll)
	btn.Size = UDim2.new(0, 200, 0, 40)
	btn.AnchorPoint = Vector2.new(0.5, 0)
	btn.Position = UDim2.new(0.5, 0, 0, 0)
	btn.Font = Enum.Font.GothamBold
	btn.TextColor3 = Color3.fromRGB(255, 255, 255)
	btn.TextSize = 16
	btn.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
	btn.Text = languages[currentLang].spawn .. " " .. name
	Instance.new("UICorner", btn).CornerRadius = UDim.new(0, 8)
	btn.MouseButton1Click:Connect(function()
		showLoading()
		print("Spawn clicked for:", name)
	end)

	btn.Parent = scroll
	table.insert(uiRefs[name], { button = btn })
	scroll.Parent = contentHolder
	return scroll
end

-- Create Tabs
-- ‡∏•‡∏ö‡∏õ‡∏∏‡πà‡∏°‡πÅ‡∏ó‡πá‡∏ö‡πÄ‡∏î‡∏¥‡∏°‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î‡∏Å‡πà‡∏≠‡∏ô‡∏™‡∏£‡πâ‡∏≤‡∏á‡πÉ‡∏´‡∏°‡πà
for _, child in ipairs(tabFrame:GetChildren()) do
	if child:IsA("TextButton") then
		child:Destroy()
	end
end

for i, name in ipairs(tabNames) do
	local btn = Instance.new("TextButton", tabFrame)
	btn.Size = UDim2.new(0, 120, 1, 0)
	btn.Position = UDim2.new(0, (i - 1) * 130, 0, 0)
	btn.Font = Enum.Font.GothamSemibold
	btn.TextSize = 14
	btn.TextColor3 = Color3.fromRGB(255, 255, 255)
	btn.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
	btn.Text = languages[currentLang].tabs[name]
	Instance.new("UICorner", btn).CornerRadius = UDim.new(0, 8)
	tabButtons[name] = btn
	tabContents[name] = createTab(name)
	btn.MouseButton1Click:Connect(function()
		for _, c in pairs(tabContents) do c.Visible = false end
		tabContents[name].Visible = true
	end)
end

-- Toggle Minimize
local toggleBtn = Instance.new("TextButton", mainFrame)
toggleBtn.Size = UDim2.new(0, 30, 0, 30)
toggleBtn.Position = UDim2.new(1, -150, 0, 10)
toggleBtn.Text = "_"
toggleBtn.Font = Enum.Font.GothamBold
toggleBtn.TextSize = 18
toggleBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
toggleBtn.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
Instance.new("UICorner", toggleBtn).CornerRadius = UDim.new(0, 6)

local isMinimized = false
toggleBtn.MouseButton1Click:Connect(function()
	isMinimized = not isMinimized
	tabFrame.Visible = not isMinimized
	contentHolder.Visible = not isMinimized
	mainFrame.Size = isMinimized and UDim2.new(0, 460, 0, 60) or UDim2.new(0, 460, 0, 480)
	toggleBtn.Text = isMinimized and "O" or "_"
end)

-- Language Update
local function updateLanguage()
	local lang = languages[currentLang]
	title.Text = lang.title
	langBtn.Text = currentLang == "en" and "‡∏†‡∏≤‡∏©‡∏≤‡πÑ‡∏ó‡∏¢" or "English"
	for name, btn in pairs(tabButtons) do
		btn.Text = lang.tabs[name]
	end
	for name, refList in pairs(uiRefs) do
		for i, ref in ipairs(refList) do
			if ref.label then ref.label.Text = lang.fields[name][i] end
			if ref.input then ref.input.PlaceholderText = lang.placeholder end
			if ref.button then ref.button.Text = lang.spawn .. " " .. name end
		end
	end
end

langBtn.MouseButton1Click:Connect(function()
	currentLang = currentLang == "en" and "th" or "en"
	updateLanguage()
end)

