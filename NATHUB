title.BackgroundTransparency = 1
title.Font = Enum.Font.GothamBlack
title.TextColor3 = Color3.fromRGB(255, 255, 255)
title.TextSize = 22
title.TextXAlignment = Enum.TextXAlignment.Left
title.Text = languages[currentLang].title

-- üåê Language Toggle
local langBtn = Instance.new("TextButton", mainFrame)
langBtn.Size = UDim2.new(0, 100, 0, 30)
langBtn.Position = UDim2.new(1, -110, 0, 10)
langBtn.Text = currentLang == "en" and "‡∏†‡∏≤‡∏©‡∏≤‡πÑ‡∏ó‡∏¢" or "English"
langBtn.Font = Enum.Font.Gotham
langBtn.TextSize = 14
